<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.application.job.model.Session" table="T_Session">

		<id column="c_session_id" name="sessionId" type="java.lang.Integer">
			<generator class="native" />
		</id>
		<property column="c_user_id" name="userId" type="java.lang.Integer" />
		<property column="c_access_token" name="accessToken" type="java.lang.String" />
		<property column="c_push_id" name="pushId" type="java.lang.String" />
		<property column="c_created" name="created" type="java.lang.Long" />
		<property column="c_modified" name="modified" type="java.lang.Long" />

	</class>
</hibernate-mapping>